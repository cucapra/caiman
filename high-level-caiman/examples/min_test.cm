#version 0.1.0

tmln time(e: Event) -> Event { returns e }
sptl space(bs: BufferSpace) -> BufferSpace { returns bs }

val min(a: i32, b: i32) -> i32 {
    returns a if a < b else b
}

fn min(a: i32, b: i32) -> i32 
    impls min, time, space
{
    if a < b {
        return a;
    } else {
        return b;
    }
}

pipeline main { min }

