version 0.0.2

type i64;
type tuple<i64>;
ref %i64l : i64-local<flags=[]>;
event %event0;
buffer_space %buffspace;
native_value %ni64 : i64;
native_value %nti64 : tuple<i64>;

function @main() -> i64;
function @foo() -> i64;


// main

value[impl default @main] %main() -> i64 {
    %y_t = call @foo();
    %y = extract %y_t 0;
    return %y;
}

timeline %time(%e : %event0) -> %event0 {
    return %e;
}

spatial %space(%bs : %buffspace) -> %buffspace {
    return %bs;
}

schedule[value $val = %main, timeline $time = %time, spatial $space = %space]
%main_head<none($time.%e)-have, none($time.%e)-have>() ->
    [%out : node($val.%y)-have none($space.%e)-have none($time.%e)-have %ni64] 
{

    %y_loc = alloc-temporary local [] i64;

    %default = default-join;
    %join = inline-join %main_ret [] %default;

    schedule-call %foo_impl[value node($val.%y_t),
        timeline output($time.%e),
        spatial none($space.%none)](%y_loc) %join;
}

schedule[value $val = %main, timeline $time = %time, spatial $space = %space]
%main_ret<none($time.%e)-have, none($time.%e)-have>
    (%y_loc : node($val.%y)-have none($space.%e)-met none($time.%e)-have %i64l) ->
    [%out : node($val.%y)-have none($space.%e)-have none($time.%e)-have %ni64] 
{
    %result = read-ref i64 %y_loc;
    return %result;
}

// foo


value[impl default @foo] %foo() -> i64 {
    %i = constant %ni64 20;
    return %i;
}

timeline %time_foo(%e : %event0) -> %event0 {
    return %e;
}

spatial %space_foo(%bs : %buffspace) -> %buffspace {
    return %bs;
}

schedule[value $val = %foo, timeline $time = %time_foo, spatial $space = %space_foo]
%foo_impl<none($time.%e)-have, none($time.%e)-have>
    (%y_loc : none($val.%i)-none none($space.%e)-met none($time.%e)-have %i64l) ->
    [%out : node($val.%i)-have none($space.%none)-met none($time.%none)-have %i64l] 
{
    local-do-builtin node($val.%i)() -> %y_loc;
    return %y_loc;
}



pipeline "main" = %main_head;