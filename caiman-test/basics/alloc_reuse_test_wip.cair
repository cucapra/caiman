// equivalent to the C++ code (to test reuse of temp memory)
long addtwo(long x) {
    long* loc = new long;
    *loc = x;
    *loc = *loc + 1;
    *loc = *loc + 1;
    return *loc;
}

version 0.0.2

type i64;
event %event0;
buffer_space %buffspace;
native_value %ni64 : i64;

function @addone(i64) -> i64;
function @main(i64) -> i64;

