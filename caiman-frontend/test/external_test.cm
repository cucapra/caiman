timeline time(e: event): event { returns e. }
spatial space(bs: bufferspace): bufferspace { returns bs. }


// explicit function class
// otherwise a spec funclet is given its own function class with
// the same name
feq op {
    extern(cpu, pure) op(i64, i64) -> i64

    extern(cpu, pure) op2(i64, i64) -> i64
}

val main() -> i64 {
    returns op(2, 10).
}

fn foo() -> i64 
    impls op, time, space
{
    return op(2, 10);
}


fn bar() -> i64
    impls op, time, space
{
    return op2(2, 10);
}

pipeline main { foo }