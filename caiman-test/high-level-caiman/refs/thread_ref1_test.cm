#version 0.1.0

tmln time(e: Event) -> Event { returns e }
sptl space(s: BufferSpace) -> BufferSpace { returns s }

val main() -> i64 {
    c :- 1
    returns id(c)
}

val id(a: i64) -> i64 {
    returns a
}


fn main_func() -> i64 impls main, time, space  
{
    var c = 1;
    let r @ none(space)-save = ref_id(&c);
    *r
}

fn ref_id(a: &i64 @ none(space)-save) -> &i64 @ none(space)-save
    impls id, time, space
{
    a
}

pipeline main { main_func }