#version 0.1.0

tmln time(e: Event) -> Event { returns e }
sptl space(bs: BufferSpace) -> BufferSpace { returns bs }


val main() -> i64 {
    b :- false
    c :- true
    d :- false
    one :- 1
    two :- 2
    r :- c if b else d
    z :- two if r else one
    returns z
}

fn foo() -> i64 @ node(val.z)-usable 
    impls main, time, space
{
    let b: bool @ node(val.b)-usable = false;
    var v: i64 @ none(val)-dead;
    if @ node(val.r)-? b {
        var r: bool @ none(val)-dead;
        let c: bool @ node(val.c)-usable = true;
        r @ node(val.r)-usable = c;
        if @ node(val.z)-? r {
            let two: i64 @ node(val.two)-usable = 2;
            v = two;
        } else {
            let one: i64 @ node(val.one)-usable = 1;
            v = one;
        }
        @in { v: [node(val.z)-usable, none(sptl)-saved] };
        @out { v: [node(val.z)-usable, none(sptl)-saved] };
    } else {
        var r: bool @ none(val)-dead;
        let d: bool @ node(val.d)-usable = false;
        r @ node(val.r)-usable = d;
        if @ node(val.z)-? r {
            let two: i64 @ node(val.two)-usable = 2;
            v = two;
        } else {
            let one: i64 @ node(val.one)-usable = 1;
            v = one;
        }
        @in { v: [node(val.z)-usable, none(sptl)-saved] };
        @out { v: [node(val.z)-usable, none(sptl)-saved] };
    }
    v
}

pipeline main { foo }