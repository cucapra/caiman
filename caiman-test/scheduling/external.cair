version 0.0.1

types [
    i64,
    slot $slot0 {
        type : i64,
        stage : ready,
        place : local,
    },
    event $event0 {
        place : local
    },
]

external_cpu i64 @op(i64);

value i64 @foo(%0 : i64) {
    %x = constant 2i64;
    %y = call @op(%x);
    %z = extract %y 0;
    return %z;
}

extras {}

pipeline "main" = @foo;