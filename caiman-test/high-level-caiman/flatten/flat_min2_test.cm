#version 0.1.0

tmln time(e: Event) -> Event { returns e }
sptl space(bs: BufferSpace) -> BufferSpace { returns bs }

val min() -> i64 {
    returns 2 if 2 < 1 else 1
}

fn min_impl() -> i64
    impls min, time, space
{
    let a = 2;
    let r = if 2 < 1 {
        a
    } else {
        1
    };
    r
}

pipeline main { min_impl }