#version 0.1.0
tmln time(e: Event) -> Event { returns e }
sptl space(bs: BufferSpace) -> BufferSpace { returns bs }

val main() -> i64 {
    b :- true
    returns 1 if b else 2
}

fn foo() -> i64 impls main, time, space
{
    let b = true;
    if b {
        return 1;
    } else {
        return 2;
    }
}

pipeline main { foo }