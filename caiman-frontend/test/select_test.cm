timeline time(e: event) -> event { returns e. }
spatial space(bs: bufferspace) -> bufferspace { returns bs. }

// specification
// by default its own function class
val main() -> i64 {
    b :- true.
    returns 1 if b else 2.
}

fn foo() -> i64 impls main, time, space
{
    // `let` in implementation
    // :- in spec

    let b = true;
    if b {
        return 1;
    } else {
        return 2;
    }
}

pipeline main { foo }